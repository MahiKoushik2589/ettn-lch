  registerMyself() {
    if (this.data) {
      console.log("Registering User:");
      console.log("First Name:", this.data.firstName || 'N/A');
      console.log("Last Name:", this.data.lastName || 'N/A');
      console.log("Email:", this.data.email || 'N/A');
    } else {
      console.log("No user data available.");
    }
  }
